@model IEnumerable<MainApp.ViewModels.HistoriesViewModel>

@{
    ViewBag.Title = "История всех платежей";
}

<h3>@ViewBag.Title</h3>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.HistoryId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DtAccount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CtAccount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sum)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Comment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OperationDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.HistoryId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DtAccount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CtAccount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Comment)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OperationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
        </tr>
    }

</table>
